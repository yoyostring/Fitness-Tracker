{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/frontPage.jsx", "app/workouts/workouts.jsx", "app/workouts/page.jsx", "app/stats/stats.jsx", "app/stats/page.jsx", "app/nutrition/nutrition.jsx", "app/components/card.jsx", "app/components/header.jsx", "app/components/plan.jsx", "app/layout.jsx", "app/page.jsx", "app/nutrition/page.jsx"],
  "exclude": ["node_modules"]
}
